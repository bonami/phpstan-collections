includes:
    - extension.neon
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon
parameters:
    ignoreErrors:
        - '~Doing instanceof PHPStan\\Type\\Generic\\GenericObjectType is error-prone and deprecated~'
    reportUnmatchedIgnoredErrors: true
    level: 9
    paths:
        - src/
        - tests/
    tmpDir: /tmp/phpstan
