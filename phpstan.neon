includes:
    - extension.neon
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon
parameters:
    ignoreErrors:
        -
            identifier: phpstanApi.instanceofType
        - '~Method .* should return class-string but returns string.~'
    reportUnmatchedIgnoredErrors: true
    level: 9
    paths:
        - src/
        - tests/
    tmpDir: /tmp/phpstan
